{
  "name": "hexo-custom-fields",
  "version": "1.1.2",
  "description": "A Hexo plugin that allows the default Markdown source files to be used as custom fields",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/loehnertz/hexo-custom-fields.git"
  },
  "scripts": {
    "build": "npm install && uglifyjs --compress --mangle --output ./index.js -- src/index.js",
    "dev": "cp ./src/index.js ./index.js",
    "prepack": "npm run build",
    "dev:watch": "watch 'npm run dev' ./src",
    "build:watch": "watch 'npm run build' ./src"
  },
  "keywords": [
    "hexo",
    "plugin",
    "markdown",
    "md",
    "custom",
    "fields",
    "snippet",
    "include"
  ],
  "author": "Jakob LÃ¶hnertz (www.jakob.codes)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/loehnertz/hexo-custom-fields/issues"
  },
  "homepage": "https://github.com/loehnertz/hexo-custom-fields#readme",
  "engines": {
    "hexo": ">= 3.0.0"
  },
  "dependencies": {
    "uglify-js": "^3.2.0",
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "watch": "^1.0.2"
  }
}
